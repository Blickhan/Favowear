<div class="row"><h3>Categories</h3></div>
<div class="row" id="search_categories_feed">
  <ul class="categories">
    <%= render @categories %>
  </ul>
  <%= will_paginate @categories %>
</div>
<div class="row"><h3>Posts</h3></div>
<div class="row" id="search_posts_feed">
  <% if @posts.any? %>
  <ol class="posts">
    <%= render @posts %>
  </ol>
  <%= will_paginate @posts %>
  <% else %>
</div>
